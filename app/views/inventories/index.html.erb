<h1>My Inventory</h1>
<table>
<tr>
<th>BookTitle<th>
<th>Author</th>
<th>Description</th>
<th>Language</th>
<th>ISBN</th>
<th>Bookcondition</th>
</tr>
<% @inventory.book_id do |mi| %>
<tr>
<td><%= mi.title %></td>
<td><%= mi.author %></td>
<td><%= mi.description %></td>
<td><%= mi.language %></td>
<td><%= mi.isbn %></td>
<td><%= mi.condition %>
</tr>
<% end %>
</table>

